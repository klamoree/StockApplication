    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")

    <!-- other libraries -->
    <script type="text/javascript" src="@Url.Content("~/Scripts/moment.min.js")"></script>
    <script type="text/javascript" src="@Url.Content("~/Scripts/qTip/jquery.qtip.min.js")"></script>

    @Scripts.Render("~/bundles/knockout")
    @Scripts.Render("~/bundles/bindingHandlers")
    @Scripts.Render("~/bundles/ajaxlogin")
    @Scripts.Render("~/bundles/customComponents")
    @RenderSection("scripts", required: false)
</body>
</html>